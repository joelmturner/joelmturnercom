        "prepare": "husky install && panda codegen"
    },
    "dependencies": {
        "@astrojs/check": "^0.3.2",
        "@astrojs/mdx": "^2.0.1",
        "@astrojs/rss": "^4.0.1",
        "@astrojs/sitemap": "^3.0.3",
        "@astrojs/svelte": "^5.0.1",
        "@fontsource/fira-code": "^5.0.16",
        "@fontsource/fira-sans": "^5.0.18",
        "@sentry/astro": "^7.90.0",
        "astro": "^4.0.4",
        "astro-cloudinary-image": "^0.0.6",
        "astro-embed": "^0.6.1",
        "astro-expressive-code": "^0.30.1",
        "cloudinary": "^1.41.0",
        "lodash": "^4.17.21",
        "lucide-astro": "^0.299.0",
        "nanostores": "^0.9.5",
        "photoswipe": "^5.4.3",
        "svelte": "^4.2.8",
        "svelte-cloudinary": "^1.1.2",
        "typescript": "^5.3.3"
