        "@astrojs/svelte": "^5.0.3",
        "@cloudinary/url-gen": "^1.16.0",
        "@fontsource/fira-code": "^5.0.16",
        "@fontsource/fira-sans": "^5.0.18",
        "@sentry/astro": "^7.98.0",
        "astro": "^4.2.6",
        "astro-cloudinary-image": "^0.0.6",
        "astro-embed": "^0.6.1",
        "astro-expressive-code": "^0.32.2",
        "cloudinary": "^1.41.3",
        "lodash": "^4.17.21",
        "lucide-astro": "^0.316.0",
        "nanostores": "^0.9.5",
