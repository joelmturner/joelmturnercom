    "devDependencies": {
        "@pandacss/astro": "^0.21.0",
        "@pandacss/dev": "^0.21.0",
        "@types/lodash": "^4.14.202",
        "@typescript-eslint/parser": "^6.15.0",
        "animated-pandacss": "^0.0.2",
        "eslint": "^8.56.0",
        "eslint-plugin-astro": "^0.31.0",
