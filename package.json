        "eslint-plugin-prettier": "^5.1.1",
        "husky": ">=6",
        "pandacss-preset-typography": "^0.1.5",
        "prettier": "^3.1.1",
        "prettier-plugin-astro": "^0.12.2",
        "prettier-plugin-svelte": "^3.1.2"
    },
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx,astro,svelte}": [
            "npx prettier --write .",
            "npx eslint --fix ."
