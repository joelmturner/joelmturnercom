{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "next",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog"
    },
    {
      "title": "til",
      "path": "[[workspace]]/src/content/til"
    }
  ],
  "frontMatter.taxonomy.tags": [
    "404",
    "500",
    "@vx",
    "API",
    "CSS",
    "Cloudinary",
    "Component",
    "D3",
    "Data Visualization",
    "Gatsby",
    "GraphQL",
    "Media",
    "OTP",
    "React",
    "React hooks",
    "Redirect",
    "SASS",
    "SCSS",
    "SVG",
    "Svelte",
    "TypeScript",
    "VSCode",
    "VX",
    "VisX",
    "a11y",
    "accessibility",
    "admin",
    "animation",
    "career",
    "charts",
    "data",
    "debug",
    "dependencies",
    "doctor who",
    "frontend",
    "git",
    "growth",
    "handlettering",
    "layout",
    "library",
    "memory",
    "mindset",
    "motivation",
    "music",
    "netlify",
    "nextjs",
    "node",
    "practice",
    "productivity",
    "project management",
    "promise",
    "quick tip",
    "routine",
    "setTimeout",
    "sketching",
    "state",
    "static",
    "storybook",
    "tips",
    "todo",
    "ui",
    "work",
    "workflow"
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "category",
          "name": "category",
          "type": "categories"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "excerpt",
          "name": "excerpt",
          "type": "string"
        },
        {
          "title": "cover",
          "name": "cover",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.taxonomy.categories": [
    "data-vis",
    "dev",
    "editor",
    "illustration",
    "lifestyle",
    "node",
    "personal development",
    "productivity",
    "state-management",
    "types",
    "ui-library"
  ],
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.content.defaultFileType": "mdx",
  "frontMatter.framework.startCommand": "yarn dev",
  "frontMatter.preview.host": "http://localhost:3000"
}